{
  "root": "libs/donaando-ui",
  "sourceRoot": "libs/donaando-ui/src",
  "projectType": "library",
  "tags": [
    "ui",
    "react"
  ],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "libs/donaando-ui/dist",
        "tsConfig": "libs/donaando-ui/tsconfig.lib.json",
        "project": "libs/donaando-ui/package.json",
        "entryFile": "libs/donaando-ui/src/index.ts",
        "external": [
          "react/jsx-runtime"
        ],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/donaando-ui/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "format": [
          "cjs"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/donaando-ui/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/donaando-ui"
      ],
      "options": {
        "jestConfig": "libs/donaando-ui/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
